{
    "name": "reactlib",
    "version": "0.0.1",
    "description": "react ts常用组件库",
    "keywords": [
        "componentLib"
    ],
    "author": "hblvsjtu",
    "license": "MIT",
    "requires": true,
    "main": "index.js",
    "scripts": {
        "start": "sh ./script/start.sh",
        "dll": "webpack --config ./build/webpack_dll.js",
        "build": "sh ./script/build.sh",
        "commit": "npx git-cz",
        "test": "jest --coverage",
        "pretty": "pretty-quick",
        "typedoc": "typedoc --out docs --target es5 --theme minimal --mode file src --ignoreCompilerErrors",
        "genlog": "npm run typedoc && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md docs && git commit --amend --no-edit",
        "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.6",
        "@types/lodash": "^4.14.155",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "@types/react-router-dom": "^5.1.5",
        "core-js": "^3.6.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "tspaxios": "^0.0.7"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-typescript": "^7.10.1",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^25.2.3",
        "babel-loader": "^8.0.0-beta.0",
        "clean-webpack-plugin": "^3.0.0",
        "commitizen": "^4.1.2",
        "conventional-changelog-cli": "^2.0.34",
        "css-loader": "^3.5.3",
        "cz-conventional-changelog": "^3.2.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "express": "^4.17.1",
        "file-loader": "^6.0.0",
        "happypack": "^5.0.1",
        "html-webpack-plugin": "^4.3.0",
        "html-webpack-tags-plugin": "^2.0.17",
        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.0.1",
        "jest-environment-enzyme": "^7.1.2",
        "jest-enzyme": "^7.1.2",
        "less": "^3.11.1",
        "less-loader": "^6.0.0",
        "lint-staged": "^8.0.0",
        "mini-css-extract-plugin": "^0.9.0",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "source-map-loader": "^0.2.4",
        "style-loader": "^1.2.1",
        "ts-jest": "^26.1.0",
        "ts-loader": "^6.2.1",
        "tslint": "^6.1.2",
        "tslint-loader": "^3.5.4",
        "typedoc": "^0.12.0",
        "typescript": "^3.9.5",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2",
        "webpack-parallel-uglify-plugin": "^1.1.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "maxHeaderWidth": 100,
            "maxLineWidth": 100,
            "defaultType": "",
            "defaultScope": "",
            "defaultSubject": "",
            "defaultBody": "",
            "defaultIssues": "",
            "types": {
                "feat": {
                    "description": "A new feature",
                    "title": "Features"
                },
                "fix": {
                    "description": "A new fix",
                    "title": "Features"
                },
                "docs": {
                    "description": "A new docs",
                    "title": "Features"
                },
                "style": {
                    "description": "A new style",
                    "title": "Features"
                },
                "refactor": {
                    "description": "A new refactor",
                    "title": "refactor"
                },
                "test": {
                    "description": "A new test",
                    "title": "test"
                },
                "revert": {
                    "description": "A new revert",
                    "title": "revert"
                },
                "config": {
                    "description": "A new config",
                    "title": "config"
                },
                "chore": {
                    "description": "A new chore",
                    "title": "chore"
                }
            }
        }
    },
    "lint-staged": {
        "{src,test}/**/*.ts": [
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && pretty-quick --staged",
            "pre-push": "jest"
        }
    }
}
